<BlazoredMenu MenuBuilder="@MenuBuilder" />

@code {
    [Parameter]
    public InstallationModel Model { get; set; }
    [Parameter]
    public string SocietyId { get; set; }
    MenuBuilder MenuBuilder = new MenuBuilder();

    protected override void OnInitialized()
    {
        var strctures = Model.Strucutres;
        if (strctures != null)
        {

            if (strctures.CogeneratorList != null)
            {
                MenuBuilder.AddItem(1, "Cogenerator", "/installation/{societyId}/cogenerator", match: NavLinkMatch.All);
            }
        }
    }


}
